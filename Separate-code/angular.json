{
    "projects": {
      "user-experience-optimization": {
        "architect": {
          "build": {
            "configurations": {
              "production": {
                "aot": true,
                "optimization": true,
                "outputHashing": "all",
                "sourceMap": false,
                "namedChunks": false,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": true,
                "budgets": [
                  {
                    "type": "initial",
                    "maximumWarning": "2mb",
                    "maximumError": "5mb"
                  }
                ]
              }
            }
          }
        }
      }
    }
  }
  